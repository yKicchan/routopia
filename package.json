{
  "name": "routopia",
  "description": "Type-safe URL builder based on your route definitions.",
  "version": "1.1.0",
  "author": "yKicchan",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE.md"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "homepage": "https://github.com/yKicchan/routopia#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yKicchan/routopia.git"
  },
  "bugs": {
    "url": "https://github.com/yKicchan/routopia/issues"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "coverage:badge": "make-coverage-badge --report-path=./artifacts/coverage/coverage-summary.json --output-path=./artifacts/coverage/badge.svg",
    "check": "tsc --noEmit && biome check",
    "fix": "tsc --noEmit && biome check --write",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "typescript",
    "url",
    "builder",
    "route",
    "path"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@types/node": "24.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "husky": "9.1.7",
    "make-coverage-badge": "1.2.0",
    "tsdown": "0.15.5",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.3"
  },
  "packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34"
}
