{
  "name": "routype",
  "description": "Friendly and fully typed route definition library.",
  "version": "0.1.0",
  "author": "yKicchan",
  "license": "MIT",
  "files": ["dist", "package.json", "README.md", "LICENSE.md"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/yKicchan/routype#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yKicchan/routype.git"
  },
  "bugs": {
    "url": "https://github.com/yKicchan/routype/issues"
  },
  "scripts": {
    "build": "rm -rf dist && pnpm build:esbuild && pnpm build:types",
    "build:esbuild": "node ./esbuild.config.mjs",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir './dist'",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "check": "tsc --noEmit && biome check",
    "fix": "tsc --noEmit && biome check --write"
  },
  "keywords": ["route", "typed", "typescript", "path", "type-safe"],
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "query-string": "^9.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "esbuild": "^0.25.2",
    "husky": "^9.1.7",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
