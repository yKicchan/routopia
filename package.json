{
  "name": "routopia",
  "description": "Type-safe URL builder based on your route definitions.",
  "version": "0.1.4",
  "author": "yKicchan",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE.md"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "homepage": "https://github.com/yKicchan/routopia#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yKicchan/routopia.git"
  },
  "bugs": {
    "url": "https://github.com/yKicchan/routopia/issues"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "check": "tsc --noEmit && biome check",
    "fix": "tsc --noEmit && biome check --write"
  },
  "keywords": [
    "typescript",
    "url",
    "builder",
    "route",
    "path"
  ],
  "packageManager": "pnpm@10.7.1+sha256.dc514890ea719003cb7a57d6b21af24fdafadd9f171e7567eca1665d7cfcef76",
  "dependencies": {
    "query-string": "^6.14.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "husky": "^9.1.7",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
